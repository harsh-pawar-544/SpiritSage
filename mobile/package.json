{
  "name": "spiritsage-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start-tunnel": "node start-tunnel-alternative.js",
    "start-lan": "expo start --lan",
    "start-localhost": "expo start --localhost",
    "start-web": "expo start --web",
    "start-workaround": "node expo-network-workaround.js",
    "start-offline": "EXPO_OFFLINE=true EXPO_NO_TELEMETRY=true expo start --localhost --clear",
    "start-safe": "expo start --web --localhost --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "analyze": "npx expo install --fix && expo doctor",
    "clear-cache": "expo start --clear",
    "reset": "rm -rf node_modules package-lock.json && npm install",
    "tunnel-free": "expo start --localhost --clear",
    "fix-network": "node expo-network-workaround.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@sentry/react-native": "^5.15.2",
    "@supabase/supabase-js": "^2.50.0",
    "expo": "~51.0.28",
    "expo-application": "~5.9.1",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-file-system": "~17.0.1",
    "expo-image": "~1.12.15",
    "expo-image-manipulator": "~12.0.5",
    "expo-linking": "~6.3.1",
    "expo-network": "~6.0.1",
    "expo-notifications": "~0.28.16",
    "expo-router": "~3.5.23",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.27.5",
    "expo-sqlite": "~14.0.6",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
